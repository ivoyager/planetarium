[application]

config/name="I Voyager Planetarium"
run/main_scene="res://ivoyager/tree_nodes/universe.tscn"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
icon="res://jupiter-icon-128.png"
main_scene="res://ivoyager/tree_nodes/universe.tscn"
name="I Voyager Planetarium"

[autoload]

Debug="*res://ivoyager/singletons/debug.gd"
Global="*res://ivoyager/singletons/global.gd"
ProjectBuilder="*res://ivoyager/singletons/project_builder.gd"

[debug]

gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1800
window/size/height=945
window/dpi/allow_hidpi=true

[importer_defaults]

texture={
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 2,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": true,
"flags/mipmaps": true,
"flags/repeat": true,
"flags/srgb": 1,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

