[application]

config/name="I, Voyager - Planetarium"
run/main_scene="res://ivoyager/tree_nodes/universe.tscn"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://web/jupiter-144.png"
icon="res://jupiter-icon-128.png"
main_scene="res://universe.tscn"
name="I, Voyager - Planetarium"

[autoload]

IVGlobal="*res://ivoyager/singletons/global.gd"
